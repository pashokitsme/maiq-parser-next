  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'maiq-cli'",
      "cargo": {
        "args": [
          "build",
          "--bin=maiq-cli",
          "--package=maiq-cli"
        ],
        "filter": {
          "name": "maiq-cli",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'maiq-cli'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=maiq-cli",
          "--package=maiq-cli"
        ],
        "filter": {
          "name": "maiq-cli",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'maiq-parser-next'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=maiq-parser-next"
        ],
        "filter": {
          "name": "maiq-parser-next",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}